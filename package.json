    "test:e2e:report": "tsx scripts/generate-test-report.ts",
    "test:all": "npm run test:ci && npm run test:e2e",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "quality:check": "npm run type-check && npm run lint:check && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "audit:security": "tsx scripts/security-audit.ts",
    "audit:performance": "tsx scripts/performance-audit.ts",
    "audit:all": "npm run audit:security && npm run audit:performance",
    "launch:checklist": "tsx scripts/launch-checklist.ts",
    "launch:prepare": "npm run quality:check && npm run test:all && npm run audit:all && npm run launch:checklist",
    "launch:deploy": "npm run launch:prepare && npm run build && npm run deploy"
